                                    <option ng-repeat="x in myData" value="{{x.code}}">
                                        {{x.name}}
                                    </option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="input-group">
                                <button class="btn btn-success" ng-click="SearchReport()">Search</button>
                            </div>
                        </div>
                    </div><hr />

